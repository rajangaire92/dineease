  "scripts": {
    "dev:backend": "npx nx run backend:serve",
    "build:backend": "npx nx run backend:build",
    "build": "yarn build:backend",
    "db:generate": "yarn prisma generate --schema ./libs/database/prisma/schema.prisma",
    "db:push": "yarn prisma db push --schema ./libs/database/prisma/schema.prisma",
    "db:migrate": "yarn prisma migrate dev --schema ./libs/database/prisma/schema.prisma",
    "db:mig:deploy": "yarn prisma migrate deploy --schema ./libs/database/prisma/schema.prisma",
    "db:studio": "yarn prisma studio --schema ./libs/database/prisma/schema.prisma"
  },
  "private": true,
  "dependencies": {
