    "http-status-codes": "^2.3.0",
    "pino": "^9.7.0",
    "pino-http": "^10.5.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/esbuild": "21.2.0",
    "@nx/eslint": "21.2.0",
